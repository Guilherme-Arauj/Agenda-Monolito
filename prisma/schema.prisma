generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id       String    @id @default(uuid())
  name     String
  email    String    @unique
  password String
  contacts Contact[]
}

model Contact {
  id          String  @id @default(uuid())
  name        String
  age         Int?
  cpf         String? 
  phone       String
  email       String
  address     String?
  socialMedia String?
  note        String?
  category    String?
  userId      String
  user        User    @relation(fields: [userId], references: [id])
}
